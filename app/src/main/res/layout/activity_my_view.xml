<?xml version="1.0" encoding="utf-8"?>
<LinearLayout

    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.cwh.mypermission.myDefinedView.MyViewAcivity">

    <com.example.cwh.mypermission.myDefinedView.RectView
        android:id="@+id/myView"
        android:paddingTop="20dp"
        android:layout_width="100dp"
        android:layout_height="100dp"

       />

    <com.example.cwh.mypermission.myDefinedView.CircleView
        android:id="@+id/myCircleView"
        android:layout_width="100dp"
        android:layout_height="100dp"
        />

    <!--<com.example.cwh.mypermission.MyAnimView-->
        <!--android:layout_width="match_parent"-->
        <!--android:layout_height="match_parent"/>-->

    <ImageView
        android:id="@+id/image"
        android:src="@drawable/round1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>

    <ImageView
        android:id="@+id/image2"
        android:src="@drawable/round2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>

</LinearLayout>
